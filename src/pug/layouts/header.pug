+b.HEADER.header(data-aos="header-aos")
	+e.wrap.wrap
		+e.center.flex._justify._vertical
			+e.A.logo.js-svg(href="#",data-svg-src="/app/objects/logo.svg")
			+e.NAV.menu
				+e.A.link.js-scroll-to(href="#main") Главная
				+e.A.link.js-scroll-to(href="#share") Акция
				+e.A.link.js-scroll-to(href="#winners") Наши победители
				+e.A.link.js-scroll-to(href="#faq") FAQ
			+e.right.flex_vertical
				+e.A.email(href="mailto:support@oboosho.com")
					strong support@
					| oboosho.com
				+e.lang
					+e.A.lang-item._current(href="#ru")
						+e.IMG.lang-icon(src='/img/flag-ru.png')
						+e.lang-text Ru
					+e.lang-list
						+e.A.lang-item(href="#ru")
							+e.IMG.lang-icon(src='/img/flag-ru.png')
							+e.lang-text Ru
						+e.A.lang-item(href="#eng")
							+e.IMG.lang-icon(src='/img/flag-eng.png')
							+e.lang-text Eng
				+e.A.btn.btn__._download.js-modal(href="#modal") 
					+e.text Скачать
					+e.load
						+e.load-icon._upstairs
							+icon('arrow','0 0 12 10')
						+e.load-icon._default
							+icon('arrow','0 0 12 10')
				+e.A.burger
